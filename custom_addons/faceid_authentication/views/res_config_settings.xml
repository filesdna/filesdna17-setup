<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

        <record id="res_config_settings_view_form_partner_geolocation" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.partner.geolocation</field>
            <field name="priority" eval="200"/>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <block id="user_default_rights" position="after">
                    <block title="Face Id Authentication" id="face_id_authentication">
                        <setting id="human_backend" title="Backend">
                            <field name="human_backend"/>
                        </setting>
                        <setting id="human_similarity" title="Backend">
                            <field name="human_similarity"/>
                        </setting>
                        <setting id="human_blinkdetection" title="Backend">
                            <field name="human_blinkdetection"/>
                        </setting>
                        <setting id="human_facingcenter" title="Backend">
                            <field name="human_facingcenter"/>
                        </setting>
                        <setting id="human_lookingcenter" title="Backend">
                            <field name="human_lookingcenter"/>
                        </setting>
                        <setting id="human_antispoofcheck" title="Backend">
                            <field name="human_antispoofcheck"/>
                        </setting>
                        <setting id="human_livenesscheck" title="Backend">
                            <field name="human_livenesscheck"/>
                        </setting>                        
                    </block>
                </block>
            </field>
        </record>

	</data>		
</odoo>