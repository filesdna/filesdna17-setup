<odoo>
    <data>

        <record id="purchase_account_project_form" model="ir.ui.view">
            <field name="name">purchase.account.project.form</field>
            <field name="model">Purchase Account</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="project_id"/>
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>