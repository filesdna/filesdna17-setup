<odoo>
    <data>
        <record id="do_not_track_models_form_view" model="ir.ui.view">
            <field name="name">do.not.track.models.form.view</field>
            <field name="model">do.not.track.models</field>
            <field name="arch" type="xml">
                <form duplicate="0">
                    <sheet>
                        <group>
                            <group string="Ir Model">
                                <field name="res_model"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="do_not_track_models_tree_view" model="ir.ui.view">
            <field name="name">do.not.track.models.tree.view</field>
            <field name="model">do.not.track.models</field>
            <field name="arch" type="xml">
                <tree duplicate="0">
                    <field name="res_model" />
                </tree>
            </field>
        </record>
        <record id="do_not_track_models_search_view" model="ir.ui.view">
            <field name="name">do.not.track.models.search.view</field>
            <field name="model">do.not.track.models</field>
            <field name="arch" type="xml">
                <search>
                    <field name="res_model" />
                </search>
            </field>
        </record>
        <record id="action_do_not_track_models_view" model="ir.actions.act_window">
            <field name="name">Do Not Track Models</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">do.not.track.models</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>